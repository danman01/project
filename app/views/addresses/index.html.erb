<h1>Listing addresses</h1>

<table>
  <tr>
    <th>City</th>
    <th>Region</th>
    <th>Country</th>
    <th>Zip</th>
    <th>Lat</th>
    <th>Lng</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @addresses.each do |address| %>
  <tr>
    <td><%= address.city_id %></td>
    <td><%= address.region_id %></td>
    <td><%= address.country_id %></td>
    <td><%= address.zip_id %></td>
    <td><%= address.lat %></td>
    <td><%= address.lng %></td>
    <td><%= link_to 'Show', address %></td>
    <td><%= link_to 'Edit', edit_address_path(address) %></td>
    <td><%= link_to 'Destroy', address, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Address', new_address_path %>
