<p class="ajax_container"><%=link_to_unless((params[:venue_scope].nil? || params[:venue_scope]=='all'),'All my venues', "/venues/user_venues?venue_scope=all", :remote=>true)%> | <%=link_to_unless(params[:venue_scope]=="upcoming", 'upcoming venues', "/venues/user_venues?venue_scope=upcoming", :remote=>true)%></p>
<table>
	<thead>
		<tr>
			<td>ID</td>
			<td>Name</td>
			<td>Capacity</td>
			<td>City</td>
			<td>Region</td>
			<td>Country</td>
			<td>Lat</td>
			<td>Longitude</td>
			
			
		</tr>
	</thead>
	<tbody>
		<%for venue in @venues.uniq!%>
		<tr>
			<td><%=venue.id%></td>
			<td><%=venue.name%></td>
			<td><%=venue.capacity%></td>
			<td><%=link_to venue.city.name, "/venues/city/#{venue.city}" unless venue.city.nil?%></td>
			<td><%=link_to venue.region.region_code, "/venues/region/#{venue.region}" unless venue.region.nil?%></td>
			<td><%=link_to venue.country.name, "/venues/region/#{venue.region}" unless venue.country.nil?%></td>
			<td><%=link_to venue.latitude, "/venues/map/#{venue.latitude}/#{venue.longitude}"%></td>
			<td><%=link_to venue.longitude, "/venues/map/#{venue.latitude}/#{venue.longitude}"%></td>
			
			
		</tr>
		<%end%>
	</tbody>
</table>