<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @venue.name %>
</p>

<p>
  <b>Capacity:</b>
  <%= @venue.capacity %>
</p>

<p>
  <b>Url:</b>
  <%= @venue.url %>
</p>

