<!DOCTYPE html>
<html>
<head>
  <title>Tickets</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery.min.js','jquery-ui.min.js','jquery_ujs.js','application.js', %>

  <%= csrf_meta_tag %>
	<%=yield :javascript%>
<style type="text/css">
#header div, #header p
{
	display:inline;
}
</style>
</head>
<body>
		<%if flash[:notice]%>
		<div id="notice">
		
		<%=flash[:notice]%>
		</div>
		
		<%end%>
		<%if flash[:error]%>
		<div id="notice">
		
		<%=flash[:error]%>
		</div>
		
		<%end%>
		<%if flash[:alert]%>
		<div id="notice">
		
		<%=flash[:alert]%>
		</div>
		
		<%end%>
		
	<div id="user_nav">
		<% if user_signed_in? %>
			Signed in as <%= link_to current_user.email, "/users/#{current_user.id}"%>. Not you?
			<%= link_to "sign out", destroy_user_session_path%>
		<%else%>
			<%= link_to "Sign up", '/users/sign_up' %> or <%=link_to 'Sign in', new_user_session_path %>
		<%end%>
	</div>
	
<%=link_to 'home','/'%>
<div id="header">
<div>
	Events
</div>
<p>
<%= link_to "View All", events_path %> / 	<%= link_to "New", new_event_path %> 
</p>
<div>
	Artists
<p>
	<%= link_to "View All", artists_path %> / <%= link_to "New", new_artist_path %>
</p>
</div>
<div>
	Tickets
<p>
	<%= link_to "View All", tickets_path %> / <%= link_to "New", new_ticket_path %>
</p>
</div>
<div>
	Sales
<p>
	<%= link_to "View All", sales_path %> / <%= link_to "New", new_sale_path %>
</p>
</div>
<div>
	Venues
<p>
	<%= link_to "View All", venues_path %> / <%= link_to "New", new_venue_path %>
</p>
</div>
<div>
	<%=form_tag "/search/results", :method=>"post" do%>
	<%=label_tag "enter artist name"%>
	<%=text_field_tag "name"%>
	<%=submit_tag 'search!'%>
	<%end%>
   ~*~*~*~*~
	<%=form_tag "/search/location_calendar", :method=>"post" do%>
	<%=label_tag "enter city name"%>
	<%=text_field_tag "city"%>
	<%=submit_tag 'search!'%>
	<%end%>
</div>
</div>
<%= yield %>

</body>
</html>
